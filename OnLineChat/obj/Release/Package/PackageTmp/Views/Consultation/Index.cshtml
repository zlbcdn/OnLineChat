@model OnLineChatDomain.LoginSessionModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>

    <link href="~/Scripts/styles/consultation.css" rel="stylesheet" />
    <link href="~/Scripts/styles/jquery-ui.css" rel="stylesheet" />
    <link href="~/Scripts/styles/uploadifive.css" rel="stylesheet" />

</head>
<body>
    
    <input type="text" id="current_id" style="display:none"   value=@Model.LoginID>
    <input type="text" id="consultation_id" style="display:none"  >

    <div class="consultation_operate">
        <span id="consultation_btn">咨询</span>
    </div>
    <h3>我的咨询列表</h3>
    
    <div class="consultation_list">
        <ul id="consultation_list_ul" class="my_fav_list">
        </ul>
    </div>


    <div id="consultataion_diag_y" style="display:none">
        <div>
            <p> 标题 </p><input type="text" id="input_title" style="width:400px">
        </div>
        <div>
            <p>详细内容</p> <textarea  id="input_content" style="width:400px;min-height:100px;height:150px"></textarea>
        </div>
        <div>
            附件<br />
            <span id="file_list" style="display:none"></span>
            <span id="file_name_list"></span>
        </div>
        <div>
            <span id="upload_img_btn">上传图片</span>
            <span id="consultation_commit">提交</span>
        </div>

    </div>

    <div id="upload_div_diag" style="display:none">
        <div id="queue"></div>
        <input id="file_upload" name="file_upload" type="file">
    </div>


    <script src="~/Scripts/chatScript/jquery.min.js"></script>
    <script src="~/Scripts/chatScript/jquery-ui.js" type="text/javascript"></script>
    <script src="~/Scripts/chatScript/jquery.uploadifive.js"></script>
    <script src="~/Scripts/chatScript/consultation.js" type="text/javascript"></script>
</body>
</html>
